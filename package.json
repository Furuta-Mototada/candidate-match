{
	"name": "candidate-match",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"scrape:kantei": "tsx ./scripts/scrape_kantei.ts",
		"scrape:sangiin": "tsx ./scripts/scrape_sangiin.ts",
		"scrape:shugiin": "tsx ./scripts/scrape_shugiin.ts",
		"scrape:member-groups": "tsx ./scripts/scrape_member_groups.ts",
		"calculate:legislation": "tsx ./scripts/calculate_legislation_scores.ts",
		"calculate:vectors": "tsx ./scripts/calculate_member_vectors.ts",
		"embeddings:generate": "python3 ./scripts/generate_bill_embeddings.py",
		"cluster:bills": "python3 ./scripts/cluster_bills.py"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"cheerio": "^1.1.2",
		"dotenv": "^16.6.1",
		"postgres": "^3.4.7",
		"undici": "^5.29.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.0",
		"@inlang/paraglide-js": "^2.4.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.16",
		"@types/node": "^22.18.13",
		"@vitest/browser": "^3.2.4",
		"drizzle-kit": "^0.31.6",
		"drizzle-orm": "^0.44.7",
		"eslint": "^9.39.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"mdsvex": "^0.12.6",
		"playwright": "^1.56.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"publint": "^0.3.15",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.16",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"vite": "^7.1.12",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"@tailwindcss/oxide"
		]
	}
}
